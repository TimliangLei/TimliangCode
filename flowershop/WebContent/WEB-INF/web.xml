<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>flowershop</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>filterCharset</filter-name>
    <filter-class>setFilter.filterCharset</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>filterCharset</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>filterLogin</filter-name>
    <filter-class>setFilter.filterLogin</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>filterLogin</filter-name>
    <url-pattern>/Cart.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>filterLogin</filter-name>
    <url-pattern>/Check.jsp</url-pattern>
  </filter-mapping>
</web-app>